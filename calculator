<html>
   <body>   
      <script type="text/javascript">
            var a = 33;
            var b = 10;
            var linebreak = "<br />";
         
            document.write("a + b = ");
            result = a + b;
            document.write(result);
            document.write(linebreak);
         
            document.write("a - b = ");
            result = a - b;
            document.write(result);
            document.write(linebreak);
            document.write("a / b = ");
            result = a / b;
            document.write(result);
            document.write(linebreak);
         
            document.write("a % b = ");
            result = a % b;
            document.write(result);
            document.write(linebreak);
         
            a = ++a;
            document.write("++a = ");
            result = ++a;
            document.write(result);
            document.write(linebreak);
         
            b = --b;
            document.write("--b = ");
            result = --b;
            document.write(result);
            document.write(linebreak);

           
        //Assignment operator
           let x = 10;
           x += 5;
           document.write("Value of x is: " + x);
            document.write(linebreak);

        
           let text = "Hello";
           text += " World";
           document.write("text");
            document.write(linebreak);

             let y = 10;
              y -= 5;
             document.write("Value of y is: " + y);
            document.write(linebreak);

            let z = 10;
            z *= 5;
            document.write("Value of x is: " + z);
      </script>
</body>
</html>

         

         
